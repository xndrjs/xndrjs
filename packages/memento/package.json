{
  "name": "@xndrjs/memento",
  "version": "1.0.0-alpha.0",
  "description": "Memento pattern implementation for undo/redo functionality with framework-agnostic state management.",
  "keywords": [
    "memento",
    "undo",
    "redo",
    "state-management",
    "framework-agnostic",
    "typescript",
    "history"
  ],
  "author": "Fabio Fognani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xndrjs/xndrjs.git",
    "directory": "packages/memento"
  },
  "homepage": "https://xndrjs.dev/",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "lint": "eslint .",
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prepublish": "pnpm format && pnpm lint && pnpm test && pnpm build && pnpm pack"
  },
  "dependencies": {
    "@xndrjs/core": "workspace:*",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@xndrjs/config-eslint": "workspace:*",
    "@xndrjs/config-typescript": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "jsdom": "^27.0.0",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
