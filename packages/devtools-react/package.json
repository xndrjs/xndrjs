{
  "name": "@xndrjs/devtools-react",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "lint": "eslint .",
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prepublish": "pnpm format && pnpm lint && pnpm test && pnpm build && pnpm pack"
  },
  "dependencies": {
    "@xndrjs/fsm": "workspace:*",
    "@xndrjs/memento": "workspace:*",
    "@xndrjs/core": "workspace:*"
  },
  "devDependencies": {
    "@xndrjs/config-eslint": "workspace:*",
    "@xndrjs/config-typescript": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.14.10",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "jsdom": "^27.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  }
}
